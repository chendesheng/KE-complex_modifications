  "rules": [
    {
      "description": "Click help menu",
      "manipulators": [
        {
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "osascript -l AppleScript /Users/chendesheng/clickHelpMenu.scpt"
            }
          ],
          "type": "basic"
        }
      ]
    },
